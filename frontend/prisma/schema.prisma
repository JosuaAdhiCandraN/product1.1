generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model bawang_merah {
  id       Int     @id @default(autoincrement())
  harga    Decimal @db.Decimal(10, 2)
  id_waktu Int
  waktu    waktu   @relation(fields: [id_waktu], references: [id_waktu], onDelete: NoAction, onUpdate: NoAction)
}

model bawang_putih {
  id       Int     @id @default(autoincrement())
  harga    Decimal @db.Decimal(10, 2)
  id_waktu Int
  waktu    waktu   @relation(fields: [id_waktu], references: [id_waktu], onDelete: NoAction, onUpdate: NoAction)
}

model beras {
  id       Int     @id @default(autoincrement())
  harga    Decimal @db.Decimal(10, 2)
  id_waktu Int
  waktu    waktu   @relation(fields: [id_waktu], references: [id_waktu], onDelete: NoAction, onUpdate: NoAction)
}

model cabai {
  id       Int     @id @default(autoincrement())
  harga    Decimal @db.Decimal(10, 2)
  id_waktu Int
  waktu    waktu   @relation(fields: [id_waktu], references: [id_waktu], onDelete: NoAction, onUpdate: NoAction)
}

model deflasi {
  id             Int     @id @default(autoincrement())
  nama_komoditas String  @db.VarChar(50)
  deflasi_rate   Decimal @db.Decimal(5, 4)
}

model inflasi {
  id             Int     @id @default(autoincrement())
  nama_komoditas String  @db.VarChar(50)
  inflasi_rate   Decimal @db.Decimal(5, 4)
}

model kangkung {
  id       Int     @id @default(autoincrement())
  harga    Decimal @db.Decimal(10, 2)
  id_waktu Int
  waktu    waktu   @relation(fields: [id_waktu], references: [id_waktu], onDelete: NoAction, onUpdate: NoAction)
}

model kedelai {
  id       Int     @id @default(autoincrement())
  harga    Decimal @db.Decimal(10, 2)
  id_waktu Int
  waktu    waktu   @relation(fields: [id_waktu], references: [id_waktu], onDelete: NoAction, onUpdate: NoAction)
}

model kentang {
  id       Int     @id @default(autoincrement())
  harga    Decimal @db.Decimal(10, 2)
  id_waktu Int
  waktu    waktu   @relation(fields: [id_waktu], references: [id_waktu], onDelete: NoAction, onUpdate: NoAction)
}

model ketimun {
  id       Int     @id @default(autoincrement())
  harga    Decimal @db.Decimal(10, 2)
  id_waktu Int
  waktu    waktu   @relation(fields: [id_waktu], references: [id_waktu], onDelete: NoAction, onUpdate: NoAction)
}

model sawi {
  id       Int     @id @default(autoincrement())
  harga    Decimal @db.Decimal(10, 2)
  id_waktu Int
  waktu    waktu   @relation(fields: [id_waktu], references: [id_waktu], onDelete: NoAction, onUpdate: NoAction)
}

model simulasi_prediksi {
  id             Int      @id @default(autoincrement())
  harga_prediksi Decimal? @db.Decimal(10, 2)
}

model tomat {
  id       Int     @id @default(autoincrement())
  harga    Decimal @db.Decimal(10, 2)
  id_waktu Int
  waktu    waktu   @relation(fields: [id_waktu], references: [id_waktu], onDelete: NoAction, onUpdate: NoAction)
}

model waktu {
  id_waktu     Int            @id @default(autoincrement())
  tanggal      DateTime?      @db.Date
  bawang_merah bawang_merah[]
  bawang_putih bawang_putih[]
  beras        beras[]
  cabai        cabai[]
  kangkung     kangkung[]
  kedelai      kedelai[]
  kentang      kentang[]
  ketimun      ketimun[]
  sawi         sawi[]
  tomat        tomat[]
}
